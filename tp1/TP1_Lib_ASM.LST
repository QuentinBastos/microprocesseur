                     276     ; Gestion du Timer 3
                     277     ; Routines permettant de mettre en oeuvre le timer 3
                     278     ;
                     279     ;******************************************************************************
                     280     
                     281     ;******************************************************************************            
                                 
                     282     ; Config_Timer3_BT
                     283     ;
                     284     ; Description: Sous-programme configurant le Timer3 pour fonctionner en
                     285     ;              Re-chargement automatique. Le timer va générer un overflow 
                     286     ;              toutes les ((65535-N) * 0.542) micro-secondes. 
                     287     ;
                     288     ; Paramètres d'entrée:  N dans R6(MSB) et R7(LSB)
                     289     ; Paramètres de sortie: aucun
                     290     ; Registres modifiés: TMR3RLL,TMR3RLH,TMR3H,TMR3L,TMR3CN
                     291     ; Pile: 0 octet (sauf pour l'appel de la sous -routine)
                     292     ;******************************************************************************
                     293     ; ATTENTION!!!   Il est impératif de remettre à zéro, par programme, le 
                     294     ;*************   Timer3 Overflow Flag.  
                     295     ;******************************************************************************
                     296     
0000                 297     Config_Timer3_BT:
                     298     
                     299     
0000 8F92            300              mov TMR3RLL, R7   ; Timer 3 Reload Register Low Byte
0002 8E93            301              mov TMR3RLH, R6   ; Timer 3 Reload Register High Byte
0004 8E95            302              mov TMR3H, R6     ; Ti mer 3 High Byte
0006 8F94            303              mov TMR3L, R7     ; Timer 3 Low Byte
0008 759106          304              mov TMR3CN, #006h ; Timer 3 Control Register
000B 22              305              ret
                     306     
